{
  "name": "@dr.pogodin/react-starter",
  "version": "0.1.0",
  "description": "ReactJS Starting Pack by Dr. Pogodin",
  "main": "index.js",
  "scripts": {
    "build": "npm run clean && npm run build:server:prod && BABEL_ENV=production ./node_modules/.bin/webpack --env=production --progress --profile --colors --display-optimization-bailout",
    "build:dev": "npm run clean && npm run build:server && BABEL_ENV=development ./node_modules/.bin/webpack --env=development --progress --profile --colors --display-optimization-bailout",
    "build:server:prod": "BABEL_ENV=production babel src --out-dir build --minified --source-maps --copy-files & BABEL_ENV=production babel src --extensions '.svg' --keep-file-extension --minified --out-dir build --source-maps",
    "clean": "./node_modules/.bin/rimraf build",
    "dev": "cross-env BABEL_ENV=development NODE_ENV=development babel-node --extensions '.js','.jsx','.svg' src/server",
    "fix:js": "./node_modules/.bin/eslint --fix --ext .js,.jsx .",
    "fix:styles": "./node_modules/.bin/stylelint **/*.scss --syntax scss --fix",
    "jest": "cross-env NODE_CONFIG_ENV=test ./node_modules/.bin/jest --config config/jest/default.js",
    "lint": "npm run lint:js && npm run lint:scss",
    "lint:js": "./node_modules/.bin/eslint --ext .js,.jsx .",
    "lint:scss": "./node_modules/.bin/stylelint **/*.scss --syntax scss",
    "start": "cross-env BABEL_ENV=production NODE_ENV=production node build/server",
    "test": "npm run lint && npm run jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/birdofpreyru/react-starter.git"
  },
  "keywords": [
    "JavaScript",
    "JS",
    "Dr. Pogodin",
    "React",
    "ReactJS",
    "Starting Pack"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/birdofpreyru/react-starter/issues"
  },
  "homepage": "https://github.com/birdofpreyru/react-starter#readme",
  "dependencies": {
    "@babel/register": "^7.5.5",
    "@dr.pogodin/react-utils": "^0.10.4",
    "cross-env": "^5.2.0",
    "lodash": "^4.17.15",
    "raf": "^3.4.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-router-dom": "^5.0.1",
    "serialize-javascript": "^1.8.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@dr.pogodin/babel-preset-svgr": "^1.0.0",
    "autoprefixer": "^9.6.1",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-css-modules-transform": "^1.6.2",
    "babel-plugin-dynamic-import-node": "^2.3.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-plugin-react-css-modules": "^5.2.6",
    "babel-plugin-transform-assets": "^1.0.2",
    "css-loader": "^3.2.0",
    "eslint": "^6.2.2",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-import-resolver-babel-module": "^5.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.15.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^2.0.1",
    "file-loader": "^4.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "jsdoc-to-markdown": "^5.0.0",
    "mini-css-extract-plugin": "^0.8.0",
    "mkpath": "^1.0.0",
    "mockdate": "^2.0.5",
    "node-sass": "^4.12.0",
    "nodelist-foreach-polyfill": "^1.2.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "postcss-scss": "^2.0.0",
    "react-hot-loader": "^4.12.11",
    "react-test-renderer": "^16.9.0",
    "resolve-url-loader": "^3.1.0",
    "rimraf": "^3.0.0",
    "sass-loader": "^7.3.1",
    "stylelint": "^10.1.0",
    "stylelint-config-standard": "^18.3.0",
    "webpack": "^4.39.2",
    "webpack-cli": "^3.3.7",
    "webpack-dev-middleware": "^3.7.0",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^4.2.1",
    "webpack-stats-plugin": "^0.3.0",
    "workbox-webpack-plugin": "^4.3.1"
  }
}
