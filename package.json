{
  "name": "@dr.pogodin/react-starter",
  "version": "0.0.0",
  "description": "ReactJS Starting Pack by Dr. Pogodin",
  "main": "index.js",
  "scripts": {
    "build": "node_modules/.bin/react-utils-build -t production",
    "build:dev": "node_modules/.bin/react-utils-build -t development",
    "dev": "cross-env BABEL_ENV=development NODE_ENV=development babel-node --extensions '.js','.jsx','.svg' src/server",
    "fix:js": "./node_modules/.bin/eslint --fix --ext .js,.jsx .",
    "fix:styles": "./node_modules/.bin/stylelint **/*.scss --syntax scss --fix",
    "jest": "cross-env NODE_CONFIG_ENV=test ./node_modules/.bin/jest --config config/jest/default.js",
    "lint": "npm run lint:js && npm run lint:scss",
    "lint:js": "./node_modules/.bin/eslint --ext .js,.jsx .",
    "lint:scss": "./node_modules/.bin/stylelint **/*.{css,scss} --syntax scss",
    "start": "cross-env NODE_ENV=production node build/server",
    "test": "npm run lint && npm run jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/birdofpreyru/react-starter.git"
  },
  "keywords": [
    "JavaScript",
    "JS",
    "Dr. Pogodin",
    "React",
    "ReactJS",
    "Starting Pack"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/birdofpreyru/react-starter/issues"
  },
  "homepage": "https://github.com/birdofpreyru/react-starter#readme",
  "engines": {
    "node": "^12.18.3",
    "npm": "^6.14.6"
  },
  "dependencies": {
    "@dr.pogodin/react-utils": "^1.8.5",
    "cross-env": "^7.0.3",
    "react": "^17.0.2",
    "react-router-dom": "^5.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.4",
    "@babel/core": "^7.15.5",
    "@babel/node": "^7.15.4",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-react": "^7.14.5",
    "@babel/types": "^7.15.6",
    "@dr.pogodin/babel-plugin-css-modules-transform": "^1.10.0",
    "@dr.pogodin/babel-plugin-transform-assets": "^1.1.1",
    "@dr.pogodin/babel-preset-svgr": "^1.3.1",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.4.3",
    "autoprefixer": "^10.3.4",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.2.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "better-docs": "^2.3.2",
    "core-js": "^3.17.3",
    "css-loader": "^6.2.0",
    "css-minimizer-webpack-plugin": "^3.0.2",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-import-resolver-babel-module": "^5.3.1",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.25.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.2.0",
    "jsdoc": "^3.6.7",
    "lodash": "^4.17.21",
    "mini-css-extract-plugin": "^2.3.0",
    "mkpath": "^1.0.0",
    "mockdate": "^3.0.5",
    "nodelist-foreach-polyfill": "^1.2.0",
    "postcss": "^8.3.6",
    "postcss-loader": "^6.1.1",
    "postcss-scss": "^4.0.0",
    "pretty": "^2.0.0",
    "react-refresh": "^0.10.0",
    "react-test-renderer": "^17.0.2",
    "regenerator-runtime": "^0.13.9",
    "resolve-url-loader": "^4.0.0",
    "sass": "^1.39.2",
    "sass-loader": "^12.1.0",
    "sitemap": "^7.0.0",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^22.0.0",
    "supertest": "^6.1.6",
    "webpack": "^5.52.1",
    "webpack-dev-middleware": "^5.1.0",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^5.8.0",
    "workbox-core": "^6.3.0",
    "workbox-precaching": "^6.3.0",
    "workbox-webpack-plugin": "^6.3.0"
  }
