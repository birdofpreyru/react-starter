{
  "name": "@dr.pogodin/react-starter",
  "version": "0.0.0",
  "description": "ReactJS Starting Pack by Dr. Pogodin",
  "main": "index.js",
  "scripts": {
    "build": "node_modules/.bin/react-utils-build -t production",
    "build:dev": "node_modules/.bin/react-utils-build -t development",
    "dev": "cross-env BABEL_ENV=development NODE_ENV=development babel-node --extensions '.js','.jsx','.svg' src/server",
    "fix:js": "./node_modules/.bin/eslint --fix --ext .js,.jsx .",
    "fix:styles": "./node_modules/.bin/stylelint **/*.scss --syntax scss --fix",
    "jest": "cross-env NODE_CONFIG_ENV=test ./node_modules/.bin/jest --config config/jest/default.js",
    "lint": "npm run lint:js && npm run lint:scss",
    "lint:js": "./node_modules/.bin/eslint --ext .js,.jsx .",
    "lint:scss": "./node_modules/.bin/stylelint **/*.scss --syntax scss",
    "start": "cross-env NODE_ENV=production node build/server",
    "test": "npm run lint && npm run jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/birdofpreyru/react-starter.git"
  },
  "keywords": [
    "JavaScript",
    "JS",
    "Dr. Pogodin",
    "React",
    "ReactJS",
    "Starting Pack"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/birdofpreyru/react-starter/issues"
  },
  "homepage": "https://github.com/birdofpreyru/react-starter#readme",
  "engines": {
    "node": "^12.18.2",
    "npm": "^6.14.5"
  },
  "dependencies": {
    "@dr.pogodin/react-utils": "^0.23.12",
    "cross-env": "^7.0.2",
    "react": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "serialize-javascript": "^4.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.4",
    "@babel/core": "^7.10.4",
    "@babel/node": "^7.10.4",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@babel/types": "^7.10.4",
    "@dr.pogodin/babel-plugin-css-modules-transform": "^1.7.1",
    "@dr.pogodin/babel-preset-svgr": "^1.0.10",
    "autoprefixer": "^9.8.4",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-transform-assets": "^1.0.2",
    "core-js": "^3.6.5",
    "css-loader": "^3.6.0",
    "eslint": "^7.4.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-import-resolver-babel-module": "^5.1.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.18.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.5",
    "event-source-polyfill": "^1.0.15",
    "file-loader": "^6.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.1.0",
    "mini-css-extract-plugin": "^0.9.0",
    "mkpath": "^1.0.0",
    "mockdate": "^3.0.2",
    "node-sass": "^4.14.1",
    "nodelist-foreach-polyfill": "^1.2.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "postcss-scss": "^2.1.1",
    "pretty": "^2.0.0",
    "react-hot-loader": "^4.12.21",
    "react-test-renderer": "^16.13.1",
    "regenerator-runtime": "^0.13.5",
    "resolve-url-loader": "^3.1.1",
    "sass-loader": "^8.0.2",
    "sitemap": "^6.1.7",
    "stylelint": "^13.6.1",
    "stylelint-config-standard": "^20.0.0",
    "webpack": "^4.43.0",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^4.2.2",
    "webpack-stats-plugin": "^0.3.2",
    "workbox-core": "^5.1.3",
    "workbox-precaching": "^5.1.3",
    "workbox-webpack-plugin": "^5.1.3"
  }
