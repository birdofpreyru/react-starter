{
  "name": "@dr.pogodin/react-starter",
  "version": "0.0.0",
  "description": "ReactJS Starting Pack by Dr. Pogodin",
  "main": "index.js",
  "scripts": {
    "build": "node_modules/.bin/react-utils-build -t production",
    "build:dev": "node_modules/.bin/react-utils-build -t development",
    "dev": "cross-env BABEL_ENV=development NODE_ENV=development babel-node --extensions '.js','.jsx','.svg' src/server",
    "fix:js": "./node_modules/.bin/eslint --fix --ext .js,.jsx .",
    "fix:styles": "./node_modules/.bin/stylelint **/*.scss --syntax scss --fix",
    "jest": "cross-env NODE_CONFIG_ENV=test ./node_modules/.bin/jest --config config/jest/default.js",
    "lint": "npm run lint:js && npm run lint:scss",
    "lint:js": "./node_modules/.bin/eslint --ext .js,.jsx .",
    "lint:scss": "./node_modules/.bin/stylelint **/*.{css,scss}",
    "start": "cross-env NODE_ENV=production node build/server",
    "test": "npm run lint && npm run jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/birdofpreyru/react-starter.git"
  },
  "keywords": [
    "JavaScript",
    "JS",
    "Dr. Pogodin",
    "React",
    "ReactJS",
    "Starting Pack"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/birdofpreyru/react-starter/issues"
  },
  "homepage": "https://github.com/birdofpreyru/react-starter#readme",
  "engines": {
    "node": ">=12.x",
    "npm": ">=6.x"
  },
  "dependencies": {
    "@dr.pogodin/react-utils": "^1.18.4",
    "cross-env": "^7.0.3",
    "react": "^18.2.0",
    "react-router-dom": "^6.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.9",
    "@babel/core": "^7.18.9",
    "@babel/eslint-parser": "^7.18.9",
    "@babel/eslint-plugin": "^7.17.7",
    "@babel/node": "^7.18.9",
    "@babel/plugin-transform-runtime": "^7.18.9",
    "@babel/preset-env": "^7.18.9",
    "@babel/preset-react": "^7.18.6",
    "@babel/register": "^7.18.9",
    "@dr.pogodin/babel-plugin-transform-assets": "^1.1.1",
    "@dr.pogodin/babel-preset-svgr": "^1.4.2",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.7",
    "autoprefixer": "^10.4.7",
    "babel-jest": "^28.1.3",
    "babel-loader": "^8.2.5",
    "babel-plugin-module-resolver": "^4.1.0",
    "core-js": "^3.23.5",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^4.0.0",
    "eslint": "^8.20.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-import-resolver-babel-module": "^5.3.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.6.0",
    "eslint-plugin-jsx-a11y": "^6.6.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "lodash": "^4.17.21",
    "memfs": "^3.4.7",
    "mini-css-extract-plugin": "^2.6.1",
    "mkpath": "^1.0.0",
    "mockdate": "^3.0.5",
    "nodelist-foreach-polyfill": "^1.2.0",
    "postcss": "^8.4.14",
    "postcss-loader": "^7.0.1",
    "postcss-scss": "^4.0.4",
    "pretty": "^2.0.0",
    "react-refresh": "^0.14.0",
    "react-test-renderer": "^18.2.0",
    "regenerator-runtime": "^0.13.9",
    "resolve-url-loader": "^5.0.0",
    "sass": "^1.53.0",
    "sass-loader": "^13.0.2",
    "sitemap": "^7.1.1",
    "stylelint": "^14.9.1",
    "stylelint-config-standard-scss": "^5.0.0",
    "supertest": "^6.2.4",
    "webpack": "^5.73.0",
    "webpack-dev-middleware": "^5.3.3",
    "webpack-hot-middleware": "^2.25.1",
    "webpack-merge": "^5.8.0",
    "workbox-core": "^6.5.3",
    "workbox-precaching": "^6.5.3",
    "workbox-webpack-plugin": "^6.5.3"
  }
}
