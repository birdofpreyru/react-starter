{
  "name": "@dr.pogodin/react-starter",
  "version": "0.0.0",
  "description": "ReactJS Starting Pack by Dr. Pogodin",
  "main": "index.js",
  "scripts": {
    "build": "node_modules/.bin/react-utils-build -t production --webpack-config webpack.config.ts",
    "build:dev": "node_modules/.bin/react-utils-build -t development --webpack-config webpack.config.ts",
    "build:types-scss": "rimraf build/types-scss && typed-scss-modules .",
    "dev": "typed-scss-modules . -w & cross-env BABEL_ENV=development NODE_ENV=development babel-node --extensions '.ts','.tsx','.svg' src/server",
    "fix:js": "./node_modules/.bin/eslint --fix --ext .js,.jsx,.ts,.tsx .",
    "fix:styles": "./node_modules/.bin/stylelint **/*.scss --syntax scss --fix",
    "jest": "cross-env NODE_CONFIG_ENV=test ./node_modules/.bin/jest --config config/jest/default.js",
    "lint": "npm run lint:js && npm run lint:scss",
    "lint:js": "./node_modules/.bin/eslint --ext .js,.jsx,.ts,.tsx .",
    "lint:scss": "./node_modules/.bin/stylelint **/*.{css,scss}",
    "start": "cross-env NODE_ENV=production node build/server",
    "test": "npm run lint && npm run typecheck && npm run jest",
    "typecheck": "npm run build:types-scss && tsc --project __tests__/tsconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/birdofpreyru/react-starter.git"
  },
  "keywords": [
    "JavaScript",
    "JS",
    "Dr. Pogodin",
    "React",
    "ReactJS",
    "Starting Pack"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/birdofpreyru/react-starter/issues"
  },
  "homepage": "https://github.com/birdofpreyru/react-starter#readme",
  "engines": {
    "node": ">=18",
    "npm": ">=9"
  },
  "dependencies": {
    "@dr.pogodin/react-themes": "^1.5.1",
    "@dr.pogodin/react-utils": "^1.26.5",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.10",
    "react": "^18.2.0",
    "react-router-dom": "^6.19.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.0",
    "@babel/core": "^7.23.0",
    "@babel/eslint-parser": "^7.22.15",
    "@babel/eslint-plugin": "^7.22.10",
    "@babel/node": "^7.22.19",
    "@babel/plugin-transform-runtime": "^7.22.15",
    "@babel/preset-env": "^7.22.20",
    "@babel/preset-react": "^7.22.15",
    "@babel/preset-typescript": "^7.23.2",
    "@babel/register": "^7.22.15",
    "@dr.pogodin/babel-plugin-transform-assets": "^1.2.2",
    "@dr.pogodin/babel-preset-svgr": "^1.8.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.11",
    "@tsconfig/recommended": "^1.0.3",
    "@types/express": "^4.17.20",
    "@types/jest": "^29.5.6",
    "@types/react-dom": "^18.2.14",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "autoprefixer": "^10.4.16",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.1.3",
    "babel-plugin-module-resolver": "^5.0.0",
    "core-js": "^3.32.2",
    "css-loader": "^6.8.1",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "eslint": "^8.50.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-import-resolver-babel-module": "^5.3.2",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jest": "^27.4.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lodash": "^4.17.21",
    "memfs": "^4.5.0",
    "mini-css-extract-plugin": "^2.7.6",
    "mkpath": "^1.0.0",
    "mockdate": "^3.0.5",
    "nodelist-foreach-polyfill": "^1.2.0",
    "postcss": "^8.4.30",
    "postcss-loader": "^7.3.3",
    "postcss-scss": "^4.0.8",
    "pretty": "^2.0.0",
    "react-refresh": "^0.14.0",
    "react-test-renderer": "^18.2.0",
    "regenerator-runtime": "^0.14.0",
    "resolve-url-loader": "^5.0.0",
    "sass": "^1.68.0",
    "sass-loader": "^13.3.2",
    "sitemap": "^7.1.1",
    "stylelint": "^15.10.3",
    "stylelint-config-standard-scss": "^11.0.0",
    "supertest": "^6.3.3",
    "typed-scss-modules": "^7.1.4",
    "typescript": "^5.2.2",
    "webpack": "^5.88.2",
    "webpack-dev-middleware": "^6.1.1",
    "webpack-hot-middleware": "^2.25.4",
    "webpack-merge": "^5.9.0",
    "workbox-core": "^7.0.0",
    "workbox-precaching": "^7.0.0",
    "workbox-webpack-plugin": "^7.0.0"
  }
}
