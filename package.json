{
  "name": "@dr.pogodin/react-starter",
  "version": "0.0.0",
  "description": "ReactJS Starting Pack by Dr. Pogodin",
  "main": "index.js",
  "scripts": {
    "build": "node_modules/.bin/react-utils-build -t production",
    "build:dev": "node_modules/.bin/react-utils-build -t development",
    "dev": "cross-env BABEL_ENV=development NODE_ENV=development babel-node --extensions '.js','.jsx','.svg' src/server",
    "fix:js": "./node_modules/.bin/eslint --fix --ext .js,.jsx .",
    "fix:styles": "./node_modules/.bin/stylelint **/*.scss --syntax scss --fix",
    "jest": "cross-env NODE_CONFIG_ENV=test ./node_modules/.bin/jest --config config/jest/default.js",
    "lint": "npm run lint:js && npm run lint:scss",
    "lint:js": "./node_modules/.bin/eslint --ext .js,.jsx .",
    "lint:scss": "./node_modules/.bin/stylelint **/*.{css,scss}",
    "start": "cross-env NODE_ENV=production node build/server",
    "test": "npm run lint && npm run jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/birdofpreyru/react-starter.git"
  },
  "keywords": [
    "JavaScript",
    "JS",
    "Dr. Pogodin",
    "React",
    "ReactJS",
    "Starting Pack"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/birdofpreyru/react-starter/issues"
  },
  "homepage": "https://github.com/birdofpreyru/react-starter#readme",
  "engines": {
    "node": ">=12.x",
    "npm": ">=6.x"
  },
  "dependencies": {
    "@dr.pogodin/react-utils": "^1.22.0",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.6",
    "react": "^18.2.0",
    "react-router-dom": "^6.4.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.20.2",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/eslint-plugin": "^7.19.1",
    "@babel/node": "^7.20.2",
    "@babel/plugin-transform-runtime": "^7.19.6",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/register": "^7.18.9",
    "@dr.pogodin/babel-plugin-transform-assets": "^1.2.1",
    "@dr.pogodin/babel-preset-svgr": "^1.5.1",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.9",
    "autoprefixer": "^10.4.13",
    "babel-jest": "^29.3.1",
    "babel-loader": "^9.1.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "core-js": "^3.26.1",
    "css-loader": "^6.7.2",
    "css-minimizer-webpack-plugin": "^4.2.2",
    "eslint": "^8.27.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-import-resolver-babel-module": "^5.3.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.5",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "lodash": "^4.17.21",
    "memfs": "^3.4.11",
    "mini-css-extract-plugin": "^2.6.1",
    "mkpath": "^1.0.0",
    "mockdate": "^3.0.5",
    "nodelist-foreach-polyfill": "^1.2.0",
    "postcss": "^8.4.19",
    "postcss-loader": "^7.0.1",
    "postcss-scss": "^4.0.5",
    "pretty": "^2.0.0",
    "react-refresh": "^0.14.0",
    "react-test-renderer": "^18.2.0",
    "regenerator-runtime": "^0.13.10",
    "resolve-url-loader": "^5.0.0",
    "sass": "^1.56.1",
    "sass-loader": "^13.2.0",
    "sitemap": "^7.1.1",
    "stylelint": "^14.15.0",
    "stylelint-config-standard-scss": "^6.1.0",
    "supertest": "^6.3.1",
    "webpack": "^5.75.0",
    "webpack-dev-middleware": "^5.3.3",
    "webpack-hot-middleware": "^2.25.3",
    "webpack-merge": "^5.8.0",
    "workbox-core": "^6.5.4",
    "workbox-precaching": "^6.5.4",
    "workbox-webpack-plugin": "^6.5.4"
  }
}
